{
  "headers": [
    {
      "source": "/Build/*.br",
      "headers": [
        {
          "key": "Content-Encoding",
          "value": "br"
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/Build/(.*)",
      "dest": "/Build/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
